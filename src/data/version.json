{
  "version": "1.0.19",
  "data_version": "1.0.5",
  "released": "2025-12-27",
  "changelog": "Optimize caching strategy: always prefer cache when online",
  "mappings": {
    "cache_version": "chinese-vocab-v1.0.19",
    "data_version": "1.0.5"
  },
  "assets": {
    "core": {
